{
  "hosting": {
    "public": "public/minisidebar/",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ 
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/login",
        "destination": "/pages-login.html.htm"
      },
      {
        "source": "/register",
        "destination": "/register.html.htm"
      },
      {
        "source": "/dashboard/**",
        "destination": "/indexUser.html"
      },
      {
        "source": "/dashboard",
        "destination": "/indexUser.html"
      },
      {
        "source": "/admin-dashboard/**",
        "destination": "/indexAdmin.html"
      },
      {
        "source": "/admin-dashboard",
        "destination": "/indexAdmin.html"
      },
      {
        "source": "/preorder" ,
        "function": "app"
      }
    ]
  }
}
